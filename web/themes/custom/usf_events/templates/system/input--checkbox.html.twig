{% elseif 'edit-node-event-field-selector-field-location' in attributes %}
    {% set disabled = "disabled" %}
{% elseif 'edit-node-event-field-selector-field-building' in attributes %}
    {% set disabled = "disabled" %}
{% elseif 'edit-node-event-field-selector-field-group-name' in attributes %}
    {% set disabled = "disabled" %}
{% elseif 'edit-node-event-field-selector-field-contact' in attributes %}
    {% set disabled = "disabled" %}
{% elseif 'edit-node-event-field-selector-field-event-type-id' in attributes %}
    {% set disabled = "disabled" %}
{% elseif 'edit-node-event-field-selector-field-ems-booking-id' in attributes %}
    {% set disabled = "disabled" %}
{% elseif 'edit-node-event-field-selector-field-ems-reservation-id' in attributes %}
    {% set disabled = "disabled" %}
{% elseif 'edit-node-event-field-selector-field-source' in attributes %}
